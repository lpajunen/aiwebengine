{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "ES2020",
    "moduleResolution": "node",
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "strict": false,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": []
  },
  "include": [
    "scripts/**/*.js",
    "scripts/**/*.ts",
    "scripts/**/*.jsx",
    "scripts/**/*.tsx",
    "assets/**/*.d.ts"
  ],
  "exclude": ["node_modules", "target"]
}
